{
  "name": "final-project",
  "version": "1.0.0",
  "description": "Final Project",
  "main": "index.js",
  "engines": {
    "node": "8.11.1"
  },
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node ./src/server/bin/server",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon ./src/server/bin/server",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "db:setup": "cd ./src/server/ && knex migrate:latest && knex seed:run",
    "storybook": "cd ./src/client/ && start-storybook -p 3007",
    "test": "jest"
  },
  "author": "Shanawaz Islam",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "app-root-path": "^2.2.1",
    "babel-polyfill": "^6.26.0",
    "circular-json": "^0.5.9",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "final-form": "^4.18.2",
    "formik": "^1.5.8",
    "interactjs": "^1.5.3",
    "knex": "^0.19.0",
    "moment-timezone": "^0.5.26",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "serve-favicon": "^2.5.0",
    "swagger-ui-express": "^4.1.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@storybook/react": "^5.2.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.0",
    "dotenv": "^8.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-angular": "^2.4.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "express-form-data": "^2.0.10",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "mock-knex": "^0.4.7",
    "node-sass": "^4.12.0",
    "nodemon": "^1.12.1",
    "sass-loader": "^7.1.0",
    "sequelize-cli": "^5.4.0",
    "webpack": "4.35.0",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
